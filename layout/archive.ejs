<div class="archives">
    <div class="archives_count">OK!目前共计 <%= page.posts.length %> 篇日志。继续努力。</div>
    <%- console.log(page) %>
    <section>
        <div class="archives_title"><strong>2017</strong></div>
        <div class="archives_item">
            <% page.posts.forEach(function(item){%>
            <a href="<%= url_for(item.path) %>" class="archives_link">
                <span class="archives_date"><%= date(item.date, 'YYYY-MM-DD')%></span>
                <%= item.title %>
            </a>
            <% }) %>
        </div>

    </section>
</div>